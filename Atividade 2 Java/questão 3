package com.mycompany.exercicio1;

/**
 *
 * @author Aluno01
 */
public class Exercicio1 {

    public static void main(String[] args) {
        for (int i = 1; i <= 100; i++) {
            if (i % 3 == 0) {
                System.out.println(i);
            }
        }
    }
}