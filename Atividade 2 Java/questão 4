package com.mycompany.exercicio1;

/**
 *
 * @author Aluno01
 */
public class Exercicio1 {

    public static void main(String[] args) {
        for (int i = 1; i <= 10; i++) {
            System.out.println("Fatorial de " + i + " = " + fatorial(i));
        }
    }
    
    public static long fatorial(int n) {
        long resultado = 1;
        for (int j = 1; j <= n; j++) {
            resultado *= j;
        }
        return resultado;
    }
}
